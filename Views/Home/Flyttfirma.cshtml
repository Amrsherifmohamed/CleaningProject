@{
    ViewData["Title"] = "Flyttfirma";
}
<!-- Contact Form Section Start -->
<div class="container-fluid py-5">
    <div class="container">
        <div class="row align-items-end mb-4">
            <div class="col-lg-6">
                <h6 class="text-secondary font-weight-semi-bold text-uppercase mb-3">KOSTNADSFRI OFFERT</h6>
                <h1 class="section-title mb-3">Välkommen! Du har kommit helt rätt!</h1>
                <p>NumberOne Flytt & Städ är det självklara valet för dig som vill ha professionell hjälp med städ och flytt.</p>
            </div>
            <div class="col-lg-6">
                <h4 class="font-weight-normal text-muted mb-3">Vi erbjuder kostnadsfri offert för dina flyttbehov. Fyll i formuläret nedan för att få en offert.</h4>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-7 mb-5 mb-lg-0">
                <div class="contact-form">
                    <form>
                        <div class="form-row">
                            <div class="col-sm-6 control-group">
                                <input type="text" class="form-control p-4" id="kvadratmeter" placeholder="Antal kvadratmeter" required="required" data-validation-required-message="Fältet är obligatoriskt" />
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="col-sm-6 control-group">
                                <input type="email" class="form-control p-4" id="email" placeholder="Email" required="required" data-validation-required-message="Fältet är obligatoriskt" />
                                <p class="help-block text-danger"></p>
                            </div>
                        </div>
                        <div class="control-group">
                            <input type="text" class="form-control p-4" id="telefonnummer" placeholder="Telefonnummer" required="required" data-validation-required-message="Fältet är obligatoriskt" />
                            <p class="help-block text-danger"></p>
                        </div>
                        <div class="control-group">
                            <input type="text" class="form-control p-4" id="stad" placeholder="Stad" required="required" data-validation-required-message="Fältet är obligatoriskt" />
                            <p class="help-block text-danger"></p>
                        </div>
                        <div class="control-group">
                            <textarea class="form-control p-4" id="beskrivning" rows="3" placeholder="Beskriv vad ni behöver hjälp med" required="required" data-validation-required-message="Fältet är obligatoriskt"></textarea>
                            <p class="help-block text-danger"></p>
                        </div>
                        <div>
                            <button class="btn btn-primary btn-block py-3 px-5" type="submit">SKICKA</button>
                        </div>
                    </form>
                </div>
            </div>
            <div class="col-lg-5" style="min-height: 400px;">
                <div class="position-relative h-100 rounded overflow-hidden">
                    <iframe style="width: 100%; height: 100%; object-fit: cover;"
                            src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3001156.4288297426!2d-78.01371936852176!3d42.72876761954724!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x4ccc4bf0f123a5a9%3A0xddcfc6c1de189567!2sNew%20York%2C%20USA!5e0!3m2!1sen!2sbd!4v1603794290143!5m2!1sen!2sbd"
                            frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Contact Form Section End -->
<!-- Pricing Section Start -->
<div class="container mt-5">
    <h2 class="text-center mb-4">Våra fasta priser för flytt</h2>
    <p class="text-center mb-5">NumberOne flytt & städ erbjuder såväl privatpersoner som företag professionell flytthjälp till rimliga priser.</p>
    <div class="row">
        <!-- Pricing Card 1 -->
        <div class="col-lg-4 col-md-6 mb-5">
            <div class="position-relative mb-4">
                <img class="img-fluid rounded w-100" src="~/img/your-image3.jpg" alt="Image">
            </div>
            <div class="bg-light rounded p-4">
                <h5 class="font-weight-bold mb-2">Max 30 KVM</h5>
                <p class="mb-4">Pris: 3.195 kr</p>
                <a href="#" class="btn btn-primary py-2">BOKA ONLINE 10% Rabatt</a>
            </div>
        </div>
        <!-- Pricing Card 2 -->
        <div class="col-lg-4 col-md-6 mb-5">
            <div class="position-relative mb-4">
                <img class="img-fluid rounded w-100" src="~/img/your-image3.jpg" alt="Image">
            </div>
            <div class="bg-light rounded p-4">
                <h5 class="font-weight-bold mb-2">Max 40 KVM</h5>
                <p class="mb-4">Pris: 3.500 kr</p>
                <a href="#" class="btn btn-primary py-2">BOKA ONLINE 10% Rabatt</a>
            </div>
        </div>
        <!-- Pricing Card 3 -->
        <div class="col-lg-4 col-md-6 mb-5">
            <div class="position-relative mb-4">
                <img class="img-fluid rounded w-100" src="~/img/your-image3.jpg" alt="Image">
            </div>
            <div class="bg-light rounded p-4">
                <h5 class="font-weight-bold mb-2">Max 50 KVM</h5>
                <p class="mb-4">Pris: 4.045 kr</p>
                <a href="#" class="btn btn-primary py-2">BOKA ONLINE 10% Rabatt</a>
            </div>
        </div>
        <!-- Add more cards as needed following the same structure -->
    </div>
</div>
<!-- Pricing Section End -->

<!-- CTA Section -->
<div class="container-fluid bg-dark text-white text-center py-5">
    <h3>Är du i behov av en ordentlig flyttfirma?</h3>
    <p>Vi på NumberOne Flytt & Städ är en seriös firma med kunden i fokus.</p>
    <a href="#" class="btn btn-primary">Boka flyttstädning med 10% rabatt</a>
    <a href="#" class="btn btn-primary">Boka flytt med 10% rabatt</a>
    <a href="#" class="btn btn-primary">Få KOSTNADSFRI OFFERT!</a>
</div>
<!-- Flyttfirma Content End -->
